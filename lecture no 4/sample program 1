#include <iostream>
using namespace std;
main ()
{
	int number , digit;
	cout << "enter a four digit number";
	cin >> number;
	
	digit = number % 10;
	cout << digit << ",";
	
	number = number / 10;
	
	digit = number % 10;
	cout << digit << ",";
	
	number = number / 10;
	
	digit = number % 10;
	cout << digit << ",";
	
	number = number / 10;
	
	digit = number % 10;
	cout << digit << ",";
	
	number = number / 10;
	return 0;
}
